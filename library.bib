keywords = {Computer aided diagnosis,Mammography,Neural network,Opposition-based learning,Zernike moments},
number = {8},
pages = {726--735},
pmid = {21722886},
publisher = {Elsevier},
title = {{Classification of benign and malignant masses based on Zernike moments}},

volume = {41},
year = {2011}
}
