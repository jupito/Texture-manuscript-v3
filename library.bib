isbn = {0027-8424},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {Computer-Assisted,Computer-Assisted: methods,Humans,Image Processing, Computer-Assisted,Image Processing, Computer-Assisted: methods,Machine Learning,Magnetic Resonance Imaging,Male,Predictive Value of Tests,Prostatic Neoplasms,Prostatic Neoplasms: radiography},
