journal = {Magnetic Resonance in Medicine},
keywords = {Algorithms,Brain,Brain: pathology,Computer-Assisted,Computer-Assisted: standards,Data Display,Data Display: standards,Foot,Foot: anatomy {\&} histology,Humans,Image Enhancement,Image Enhancement: standards,Image Processing,Magnetic Resonance Imaging,Magnetic Resonance Imaging: standards,Multiple Sclerosis,Multiple Sclerosis: pathology,mri,standardization},
mendeley-tags = {mri,standardization},
month = {dec},
