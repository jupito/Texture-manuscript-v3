issn = {10531807},
journal = {Journal of Magnetic Resonance Imaging},
keywords = {MRI,T2-weighted imaging,computer-aided detection,prostate cancer,transition zone},
month = {aug},
number = {2},
pages = {301--305},
pmid = {24924512},
title = {{Pilot study of a novel tool for input-free automated identification of transition zone prostate tumors using T2- and diffusion-weighted signal and textural features}},

